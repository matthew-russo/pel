func main() {
  let greeter: Greeter := EnglishGreeter {};
  print(greeter.greet("matthew"));
  print(greeter.goodbye());
}

contract Greeter {
  func greet(name: string) -> string;
}

object EnglishGreeter {
  fields {

  }

  methods {
    func goodbye(self) -> string {
      return "goodbye";
    }
  }
}

implement Greeter for EnglishGreeter {
  func greet(name: string) -> string {
    return "hello " + name;
  }
}
