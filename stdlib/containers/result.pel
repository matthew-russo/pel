enum Result<<L, R>> {
  variants {
    Ok(L),
    Err(R),
  }

  methods {
    func is_ok(self) -> bool; {
      match self {
        Ok(_) => { return true; },
        _ => { return false; },
      };
    }

    func is_err(self) -> bool; {
      match self {
        Err(_) => { return true; },
        _ => { return false; },
      };
    }
  }
}

