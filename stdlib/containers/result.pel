enum Result<L, R> {
  variants {
    Ok(L),
    Err(R),
  }

  methods {
    public is_ok(self) -> bool {
      match self {
        Ok(_) => true,
        _ => false,
      }
    }

    public is_err(self) -> bool {
      match self {
        Err(_) => true,
        _ => false,
      }
    }
  }
}

